<p id="notice"><%= notice %></p>

<h1>Listing Temp Names</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temp_names.each do |temp_name| %>
      <tr>
        <td><%= temp_name.first_name %></td>
        <td><%= temp_name.last_name %></td>
        <td><%= link_to 'Show', temp_name %></td>
        <td><%= link_to 'Edit', edit_temp_name_path(temp_name) %></td>
        <td><%= link_to 'Destroy', temp_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Temp name', new_temp_name_path %>
